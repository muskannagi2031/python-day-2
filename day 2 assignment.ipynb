   "source": [
    "t = int(input(\"Enter the number\"))\n",
    "for i in range(0,t):\n",
    "    count = 0\n",
    "    for i in range(2,i*i):\n",
    "        if t%i == 0:\n",
    "            count = count + 1\n",
    "            break\n",
    "\n",
    "if count == 0:\n",
    "    print(\"Prime\")\n",
    "else:\n",
    "    print(\"Not Prime\")"
   ]
